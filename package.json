{
  "name": "testable",
  "version": "0.1.0",
  "homepage": "https://marabesi.github.io/testable/",
  "private": true,
  "dependencies": {
    "firebase": "^5.5.9",
    "firebaseui": "^3.4.1",
    "material-design-lite": "^1.2.0",
    "node-sass": "^4.11.0",
    "react": "^16.6.3",
    "react-codemirror": "^1.0.0",
    "react-dom": "^16.6.3",
    "react-ga": "^2.5.7",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.3",
    "react-transition-group": "^1.2.1",
    "react-typing-animation": "^1.6.2",
    "tailwindcss": "^0.7.4",
    "typed.js": "^2.0.10"
  },
  "scripts": {
    "debug": "export REACT_APP_DEBUG=true && npm run tailwind:css && react-scripts start",
    "start": "npm run tailwind:css && react-scripts start",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "build": "npm run tailwind:css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "tailwind:css": "tailwind build src/tailwind.src.css -c tailwind.js -o src/tailwind.css"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "gh-pages": "^2.0.1"
  }
}
