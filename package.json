{
  "name": "testable",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "cypress": "^3.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "esprima": "^4.0.1",
    "fbemitter": "^2.1.1",
    "firebase": "^5.11.1",
    "firebaseui": "^3.6.0",
    "intro.js": "^2.9.3",
    "intro.js-react": "^0.2.0",
    "material-design-lite": "^1.2.0",
    "node-sass": "^4.12.0",
    "react": "^16.8.6",
    "react-codemirror": "^1.0.0",
    "react-dom": "^16.8.6",
    "react-ga": "^2.5.7",
    "react-helmet": "^5.2.1",
    "react-router-dom": "^4.3.1",
    "react-router-transition": "^1.3.0",
    "react-scripts": "^2.1.8",
    "react-test-renderer": "^16.8.6",
    "react-typing-animation": "^1.6.2",
    "tailwindcss": "^0.7.4",
    "typed.js": "^2.0.10"
  },
  "scripts": {
    "analyze": "npm run build && source-map-explorer 'build/static/js/*.js'",
    "debug": "export REACT_APP_DEBUG=true && npm run tailwind:css && react-scripts start",
    "start": "npm run tailwind:css && react-scripts start",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "build": "npm run lint && npm run tailwind:css && export NODE_ENV=production && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "coverage": "react-scripts test --env=jsdom --collectCoverage",
    "cypress": "cypress open",
    "cypress-run": "cypress run",
    "eject": "react-scripts eject",
    "tailwind:css": "tailwind build src/tailwind.src.scss -c tailwind.js -o src/tailwind.css",
    "lint": "eslint src/**/*.js"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "eslint-plugin-react": "^7.13.0",
    "gh-pages": "^2.0.1",
    "sinon": "^7.3.2",
    "source-map-explorer": "^1.8.0"
  }
}
