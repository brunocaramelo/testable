{
  "name": "testable",
  "version": "0.1.0",
  "homepage": "https://marabesi.github.io/testable/build",
  "private": true,
  "dependencies": {
    "esprima": "^4.0.1",
    "fbemitter": "^2.1.1",
    "firebase": "^5.11.1",
    "firebaseui": "^3.6.0",
    "intro.js": "^2.9.3",
    "intro.js-react": "^0.2.0",
    "lodash": "^4.17.11",
    "material-design-lite": "^1.2.0",
    "node-sass": "^4.12.0",
    "react": "^16.8.6",
    "react-codemirror": "^1.0.0",
    "react-dom": "^16.8.6",
    "react-ga": "^2.5.7",
    "react-helmet": "^5.2.1",
    "react-redux": "^7.1.0",
    "react-router-dom": "^4.3.1",
    "react-router-transition": "^1.3.0",
    "react-scripts": "^2.1.8",
    "react-test-renderer": "^16.8.6",
    "react-typing-animation": "^1.6.2",
    "redux": "^4.0.1",
    "tailwindcss": "^0.7.4",
    "typed.js": "^2.0.10"
  },
  "scripts": {
    "post:css": "postcss -c postcss.config.js src/css/tailwind.css -o src/css/index.css",
    "analyze": "npm run build && source-map-explorer 'build/static/js/*.js'",
    "debug": "export REACT_APP_DEBUG=true && npm run tailwind:css && react-scripts start",
    "start": "npm run tailwind:css && react-scripts start",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "build": "npm run lint && npm run tailwind:css && export NODE_ENV=production && export REACT_APP_BASE_NAME=/testable/build/ && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "coverage": "react-scripts test --env=jsdom --collectCoverage",
    "cypress": "cypress open",
    "cypress-run": "cypress run",
    "eject": "react-scripts eject",
    "tailwind:css": "tailwind build src/scss/tailwind.src.scss -c src/tailwind.js -o src/css/tailwind.css && npm run post:css",
    "lint": "eslint src/**/*.js",
    "coveralls": "react-scripts test --coverage --coverageReporters=text-lcov | coveralls"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.2.0",
    "babel-eslint": "^9.0.0",
    "coveralls": "^3.0.4",
    "cypress": "^3.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint-plugin-react": "^7.13.0",
    "gh-pages": "^2.0.1",
    "postcss-cli": "^6.1.2",
    "sinon": "^7.3.2",
    "source-map-explorer": "^1.8.0"
  }
}
